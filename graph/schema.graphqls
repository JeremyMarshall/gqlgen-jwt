# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Property {
  name: String!
  value: String!
}

type Jwt {
  user: String!
  roles: [String!]!
  properties: [Property!]!
}

type Query {
  jwt(token: String!): Jwt!
}

input NewJwt {
  user: String!
  roles: [String!]!
}

type Mutation {
  createJwt(input: NewJwt!): String!
}